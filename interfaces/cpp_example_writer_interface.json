{
    "description": "Provide some variables to be shared to the CppExampleReader module",
    "vars": {
        "cpp_writer_published_var": {
            "description": "Example writer integer message",
            "type": "integer",
            "minimum": 1,
            "maximum": 180,
            "default": 1
        }
    },
    "cmds": {
        "set_tx_prescaler": {
            "description": "Sets the prescaler for message transmission",
            "arguments": {
                "tx_prescaler": {
                    "description": "Prescaler: each unit represents 500ms",
                    "type": "integer",
                    "minimum": 4,
                    "maximum": 20,
                    "default": 4
                }
            }
        }
    }
}